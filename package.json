{
  "name": "industry-ui",
  "author": "Ian Warner <ian.warner@drykiss.com>",
  "version": "0.0.1",
  "description": "React components library",
  "main": "./index.js",
  "license": "MIT",
  "repository": "git@github.com:drykiss/industry-ui.git",
  "scripts": {
    "check": "npm-check -s -u",
    "dedupe": "yarn-deduplicate yarn.lock",
    "start": "start-storybook -p 9000 --quiet -c .storybook",
    "test": "jest --coverage",
    "clean-dist": "rm -rf ./dist",
    "build": "npm run clean-dist && NODE_ENV=production `yarn bin`/babel ./src --out-dir ./dist",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "standard": {
    "globals": [
      "__SITE__"
    ],
    "ignore": [
      "node_modules/**",
      "*.bundle*.js"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/pro-regular-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@typeform/embed": "^0.12.1",
    "firebase": "^6.4.0",
    "moment": "^2.24.0",
    "next": "^9.0.0",
    "react-dropzone": "^10.1.5",
    "react-select": "^3.0.4",
    "react-tooltip": "^3.10.0",
    "slick-carousel": "^1.8.1",
    "yarn-deduplicate": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "@babel/template": "^7.4.4",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.1",
    "jest": "^24.9.0",
    "npm-check": "^5.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard": "^13.1.0",
    "storybook-readme": "^5.0.5",
    "styled-components": "^4.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1"
  }
}
